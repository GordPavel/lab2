<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="CENTER" hgap="10.0"
          prefHeight="334.0" prefWidth="474.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.112-ea"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraficApp.NewFunctionController">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="204.0" minWidth="10.0" prefWidth="178.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="238.0" minWidth="10.0" prefWidth="212.0"/>
    </columnConstraints>

    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>

    <Label text="Left domain border" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0"
           GridPane.valignment="CENTER"/>
    <Label text="Right domain border" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1"
           GridPane.valignment="CENTER"/>
    <Label text="Points count" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="2"
           GridPane.valignment="CENTER"/>

    <Label fx:id="errorTextLabel" prefHeight="15.0" prefWidth="193.0" text="errorTExt" textAlignment="CENTER"
           textFill="RED" visible="false" GridPane.rowIndex="4"/>
    <TextField fx:id="leftDomainBorderTextField" prefWidth="179.0" GridPane.columnIndex="1"
               GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER"/>
    <TextField fx:id="rightDomainBorderTextField" prefWidth="200.0" GridPane.columnIndex="1"
               GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
    <Spinner fx:id="pointsCountSpinner" initialValue="2" max="10000000" min="2"
             GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
    <Button mnemonicParsing="false" onAction="#cancelAction" prefHeight="7.0" prefWidth="83.0" scaleX="1.5"
            scaleY="1.5" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3"
            GridPane.valignment="CENTER"/>
    <Button mnemonicParsing="false" onAction="#okAction" prefHeight="10.0" prefWidth="73.0" scaleX="1.5"
            scaleY="1.5" text="OK" GridPane.columnIndex="0" GridPane.halignment="CENTER"
            GridPane.rowIndex="3" GridPane.valignment="CENTER"/>
</GridPane>
